Private Sub txt_End_Time_AfterUpdate()

If Me!txt_End_Time < Me!txt_Start_Time Then
    MsgBox "The survey end time must be greater than the survey start time.", , "Forest Bird Monitoring"
    Me!txt_End_Time.SetFocus
End If

End Sub

Private Sub txt_Start_Date_AfterUpdate()
If Me!txt_Start_Date = "" Or IsNull(txt_Start_Date) Then
            Exit Sub
        End If
    
        Dim strDate As Date
        strDate = Me!cmbo_Find_Route_Event.Column(2)
        'MsgBox strDate
        
        Dim strStartDate As Date
        strStartDate = Me!txt_Start_Date.Value
        ' MsgBox strStartDate
        Dim strEndDate As Date
        strEndDate = Me!cmbo_Find_Route_Event.Column(3)
        
         If strStartDate < strDate Then
            MsgBox "The survey date can not be less than the start date for the sampling route." & vbLf & vbLf _
                & "Please check the survey date.", vbExclamation, "Forest Bird Monitoring"
         ElseIf strStartDate > strEndDate Then
            MsgBox "The survey date can not be greater than the end date for the sampling route." & vbLf & vbLf _
                & "Please check the survey date.", vbExclamation, "Forest Bird Monitoring"
        End If

End Sub

